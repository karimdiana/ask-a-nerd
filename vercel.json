{
  "version": 2,
  "functions": {
    "api/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    { "src": "/api/chat", "dest": "/api/chat" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "handle": "filesystem" },
    { "src": "/", "dest": "/dashboard.html" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
} 